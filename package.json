{
  "name": "mc-patcher",
  "version": "1.0.0",
  "description": "A graphic tool to patch minecraft instances",
  "main": "main.js",
  "scripts": {
    "start": "electron ./",
    "build": "electron-builder -c electron-builder.json -wl",
    "clean": "rimraf build/ dist/",
    "lint": "eslint --fix ./",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run lint"
  },
  "keywords": [
    "minecraft",
    "mod",
    "patch"
  ],
  "author": "Xisabla",
  "license": "MIT",
  "devDependencies": {
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-sort-requires": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2"
  },
  "dependencies": {}
}
