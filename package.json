{
  "name": "mc-patcher",
  "version": "0.0.1",
  "description": "A graphic tool to patch minecraft instances",
  "main": "main.js",
  "scripts": {
    "start": "electron ./",
    "start:dev": "RELOAD=1 npm run start",
    "dev": "run-p build:dev start:dev",
    "build": "webpack --mode production --progress",
    "build:dev": "webpack --mode development --watch",
    "clean": "rimraf build/ dist/ coverage/",
    "lint": "npm run lint:style && npm run lint:es",
    "lint:style": "stylelint --fix src/",
    "lint:es": "eslint --fix ./src",
    "package": "electron-builder -c electron-builder.json -wl",
    "postinstall": "electron-builder install-app-deps",
    "prestart": "npm run lint && npm run build"
  },
  "keywords": [
    "minecraft",
    "mod",
    "patch"
  ],
  "author": "Xisabla",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/free-regular-svg-icons": "^5.13.1",
    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@types/bootstrap": "^4.5.0",
    "@types/dotenv": "^8.2.0",
    "@types/jquery": "^3.5.0",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-bootstrap": "^1.0.1",
    "@types/react-dom": "^16.9.8",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "bootstrap": "^4.5.0",
    "css-loader": "^3.6.0",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "electron-reload": "^1.5.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "eslint-plugin-sort-requires": "^2.1.0",
    "file-loader": "^6.0.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.0",
    "source-map-loader": "^1.0.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "ts-loader": "^7.0.5",
